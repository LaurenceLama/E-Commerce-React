book);\n  }\n\n  //removes the checkout function to unchosen books that are still not added to cart, so if chosen book is added to cart, then checkout shows, otherwise like finding other books, add to cart will display again\n  function bookOnCart() {\n    return cart.find((book) => book.id === +id);\n  } // for this called cart to work, it should also be received/returnedIthink back to App.js, FSE discord very helpful bros\n\n  return (\n    <div id=\"books__body\">\n      <main id=\"books__main\">\n        <div className=\"books__container\">\n          <div className=\"row\">\n            <div className=\"book__selected--top\">\n              <Link to=\"/books\" className=\"book__link\">\n                <FontAwesomeIcon icon=\"arrow-left\" />\n              </Link>\n              <Link to=\"/books\" className=\"book__link\">\n                <h2 className=\"book__selected--title--top\">Books</h2>\n              </Link>\n            </div>\n            <div className=\"book__selected\">\n              <figure className=\"book__selected--figure\">\n                <img className=\"book__selected--img\" src={book.url} alt=\"\" />\n              </figure>\n              <div className=\"book__selected--description\">\n                <h2 className=\"book__selected--title\">{book.title}</h2>\n                <Ratings rating={book.rating} />\n                <div className=\"book__selected--price\">\n                  <Price\n                    originalPrice={book.originalPrice}\n                    salePrice={book.salePrice}\n                  />\n                </div>\n                <div className=\"book__summary\">\n                  <h3 className=\"book__summary--title\">Summary</h3>\n                  <p className=\"book__summary--para\">\n                    Lorem ipsum dolor sit amet consectetur adipisicing elit.\n                    Veniam, repellendus modi odio porro, consequuntur,\n                    asperiores minima sint voluptatem at reiciendis ducimus\n                    neque provident alias iure nihil explicabo nobis id\n                    voluptas.\n                  </p>\n                  <p className=\"book__summary--para\">\n                    Lorem ipsum dolor sit amet consectetur adipisicing elit.\n                    Veniam, repellendus modi odio porro, consequuntur,\n                    asperiores minima sint voluptatem at reiciendis ducimus\n                    neque provident alias iure nihil explicabo nobis id\n                    voluptas.\n                  </p>\n                </div>\n                {bookOnCart() ? (\n                  <Link to={`/cart`} className=\"book__link\">\n                    <button className=\"btn\">Checkout now</button>\n                  </Link>\n                ) : (\n                  <button className=\"btn\" onClick={() => addBookToCart(book)}>\n                    Add to Cart\n                  </button>\n                )}\n              </div>\n            </div>\n          </div>\n        </div>\n        <div className=\"books__container\">\n          <div className=\"row\">\n            <div className=\"book__selected--top\">\n              <h2 className=\"book__selected--title--top\">Recommended Books</h2>\n            </div>\n            <BestBooks id={id} />\n          </div>\n        </div>\n      </main>\n    </div>\n  );\n};\n\nexport default BookInfo;\n","C:\\Users\\MAWIE18\\OneDrive\\Desktop\\Coderist\\PROJECTS_O_ewan\\project_5_reactss\\E-Commerce_REACT\\e-comms_project\\library-react-tutorial\\src\\pages\\Cart.jsx",[],[],"C:\\Users\\MAWIE18\\OneDrive\\Desktop\\Coderist\\PROJECTS_O_ewan\\project_5_reactss\\E-Commerce_REACT\\e-comms_project\\library-react-tutorial\\src\\components\\Book.jsx",[],[],{"ruleId":"111","severity":1,"message":"112","line":1,"column":27,"nodeType":"113","messageId":"114","endLine":1,"endColumn":36},{"ruleId":"111","severity":1,"message":"115","line":10,"column":10,"nodeType":"113","messageId":"114","endLine":10,"endCo